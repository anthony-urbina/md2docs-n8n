{
  "functions": {
    "source": "lib",
    "runtime": "nodejs22"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    }
  },
  "hosting": {
    "site": "md2doc-n8n-aemalsayer-docs",
    "public": "public",
    "rewrites": [
      {
        "source": "/.well-known/acme-challenge/**",
        "destination": "/.well-known/acme-challenge/**"
      },
      {
        "source": "**",
        "function": {
          "functionId": "mdToGoogleDoc",
          "region": "us-east1"
        }
      }
    ]
  }
}
