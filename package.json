{
  "name": "markdown-to-gdocs-function",
  "version": "1.0.0",
  "description": "Firebase Function to convert Markdown to Google Docs",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts --outdir=lib --target=node22 && cp package.json lib/",
    "build:watch": "esbuild src/index.ts --outdir=lib --target=node --watch",
    "serve": "pnpm run build && firebase emulators:start",
    "shell": "pnpm run build && firebase functions:shell",
    "start": "pnpm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "esbuild src/index.ts --outdir=lib --target=node --watch"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "docx": "^8.5.0",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.5.0",
    "googleapis": "^128.0.0",
    "marked": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/marked": "^5.0.2",
    "@types/node": "^18.0.0",
    "firebase-functions-test": "^3.1.0"
  },
  "private": true
}
